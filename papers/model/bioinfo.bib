  issue                  = {1},
  keywords               = {Base Composition/genetics, Biosynthetic Pathways/*genetics, DNA Transposable Elements/genetics, Genome, Bacterial/*genetics, *Genomics, *High-Throughput Nucleotide Sequencing, Molecular Sequence Annotation, Nanopores, Repetitive Sequences, Nucleic Acid/genetics, Sequence Analysis, DNA, Genome assembly, Genome sequencing, Insertion sequences, Oxford Nanopore MinION, Secondary metabolites},
  language               = {eng},
  linking-issn           = {1471-2164},
