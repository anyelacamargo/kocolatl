  issue                  = {4},
  keywords               = {Arabidopsis/*genetics/physiology, Computer Simulation, Gene Expression Profiling, *Gene Regulatory Networks, *Models, Biological},
  language               = {eng},
  linking-issn           = {1064-5462},
  location-id            = {10.1162/ARTL_a_00076 [doi]},
  nlm-unique-id          = {9433814},
  owner                  = {NLM},
  print-issn             = {1064-5462},
  publication-status     = {ppublish},
  revised                = {20121031},
