  issue                  = {8},
  keywords               = {*Carbohydrate Metabolism, Carbon Dioxide/metabolism, Cell Wall/enzymology, *Cellular Senescence, Chloroplasts/genetics/physiology, Hexokinase/metabolism, Plant Leaves/genetics/*physiology, *Plant Physiological Phenomena, Plants/genetics, Signal Transduction, beta-Fructofuranosidase/metabolism},
  language               = {eng},
  linking-issn           = {0022-0957},
  location-id            = {10.1093/jxb/ern076 [doi]},
  nlm-unique-id          = {9882906},
  owner                  = {NLM},
  publication-status     = {ppublish},
  registry-number        = {EC 3.2.1.26 (beta-Fructofuranosidase)},
  revised                = {20171116},
  source                 = {J Exp Bot. 2008;59(8):1963-72. doi: 10.1093/jxb/ern076. Epub 2008 May 2.},
  status                 = {MEDLINE},
