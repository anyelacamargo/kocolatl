  owner                  = {NLM},
  publication-status     = {epublish},
  revised                = {20200306},
  source                 = {Sci Rep. 2020 Jan 16;10(1):460. doi: 10.1038/s41598-019-57116-0.},
  status                 = {In-Data-Review},
  subset                 = {IM},
  title-abbreviation     = {Sci Rep},
}

