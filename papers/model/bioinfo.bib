  language               = {eng},
  linking-issn           = {0167-4412},
  location-id            = {10.1007/s11103-013-0013-8 [doi]},
  nlm-unique-id          = {9106343},
  owner                  = {NLM},
  publication-status     = {ppublish},
  revised                = {20181113},
