  print-issn         = {0022-0957},
  publication-status = {ppublish},
  registry-number    = {7440-44-0 (Carbon)},
  revised            = {20190513},
  source             = {J Exp Bot. 2000 Feb;51 Spec No:329-37. doi: 10.1093/jexbot/51.suppl_1.329.},
  status             = {MEDLINE},
