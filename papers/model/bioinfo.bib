  issue                  = {9},
  keywords               = {Adaptation, Biological, Alleles, Base Sequence, Chromosomes, Plant/genetics, *DNA Copy Number Variations, DNA, Plant/*genetics, Evolution, Molecular, Exons, Gene Duplication, Genome Size, *Genome, Plant, Open Reading Frames, Phenotype, Polymorphism, Single Nucleotide, Sequence Alignment, Sequence Analysis, DNA/*methods, Species Specificity, *Tetraploidy, Triticum/*genetics},
  language               = {eng},
  linking-issn           = {1474-7596},
  location-id            = {10.1186/gb-2011-12-9-r88 [doi]},
  nlm-unique-id          = {100960660},
